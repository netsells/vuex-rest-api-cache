{
  "name": "@netsells/vuex-rest-api-cache",
  "version": "1.0.1",
  "description": "Vuex Rest API action creator and cacher",
  "main": "dist/index.js",
  "scripts": {
    "test-api": "babel-node spec/server.js",
    "jest": "jest",
    "jest:watch": "jest --watch",
    "jest:mutate": "stryker run",
    "codecov": "codecov",
    "test": "start-server-and-test test-api http://localhost:3000 jest",
    "test:watch": "start-server-and-test test-api http://localhost:3000 jest:watch",
    "test:mutate": "start-server-and-test test-api http://localhost:3000 jest:mutate",
    "lint:js": "eslint src --max-warnings 0",
    "lint:spec": "eslint spec --max-warnings 0 --config .spec.eslintrc",
    "lint": "yarn lint:js && yarn lint:spec",
    "build": "babel src -d dist",
    "prepublishOnly": "yarn build"
  },
  "author": "Sam Boylett <sam.boylett@netsells.co.uk>",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.6.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "codecov": "^3.1.0",
    "eslint-plugin-jest": "^22.1.2",
    "express": "^4.16.4",
    "jest": "^23.6.0",
    "regenerator-runtime": "^0.13.1",
    "start-server-and-test": "^1.7.11",
    "stryker": "^0.33.1",
    "stryker-api": "^0.22.0",
    "stryker-babel-transpiler": "^0.7.10",
    "stryker-html-reporter": "^0.16.9",
    "stryker-javascript-mutator": "^0.12.1",
    "stryker-jest-runner": "^1.2.9",
    "vue": "^2.5.20",
    "vuex": "^3.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/netsells/vuex-rest-api-cache.git"
  },
  "jest": {
    "moduleNameMapper": {
      "^~/(.+)$": "<rootDir>/src/$1"
    },
    "testEnvironment": "node",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "dependencies": {
    "@netsells/eslint-config": "^1.8.0",
    "axios": "^0.18.0",
    "eslint": "^4.10.0"
  }
}
